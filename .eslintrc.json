{
  "root": true,
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "@nuxt/eslint-config",
    "prettier",
    "plugin:prettier/recommended"
  ],
  "plugins": ["prettier"],
  "rules": {
    "vue/no-v-html": "off",
    "vue/require-default-prop": "off"
  },
  "parserOptions": {
    "warnOnUnsupportedTypeScriptVersion": false
  }
}
